    "version": "0.2.0",
    "configurations": [
        {
            "type": "aws-sam",
            "request": "direct-invoke",
            "name": "selenium-scrap01:FnBOTPlaywright",
            "invokeTarget": {
                "target": "template",
                "templatePath": "${workspaceFolder}/template.yaml",
                "logicalId": "FnBOTPlaywright"
            },
            "lambda": {
                "payload": {},
                "environmentVariables": {}
            }
        },
        {
            "name": "api.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/api.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceFolder}/src/",
            "envFile": "${workspaceFolder}/.env.local"
        },
        {
            "name": "cli.py 1",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/cli.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceFolder}/src/",
            "envFile": "${workspaceFolder}/.env.local",
            "args": [
                "1"
            ]
        }
    ]
}